{
    "editor.fontFamily": "'CaskaydiaCove Nerd Font Mono', 'Courier New', monospace",
    "editor.codeLensFontFamily": "'CaskaydiaCove Nerd Font Mono'",
    "[typescriptreact]": {
        "editor.tabSize": 2,
        "editor.defaultFormatter": "esbenp.prettier-vscode",
    },
    "[typescript]": {
        "editor.tabSize": 2,
        "editor.defaultFormatter": "esbenp.prettier-vscode",
    },
    "[javascript]": {
        "editor.tabSize": 2,
        "editor.defaultFormatter": "esbenp.prettier-vscode",
    },
    "[css]": {
        "editor.tabSize": 2,
    },
    "[json]": {
        "editor.tabSize": 2,
        "editor.defaultFormatter": "esbenp.prettier-vscode",
    },
    "[markdown]": {
        "editor.wordWrap": "bounded",
        "editor.wordWrapColumn": 120,
    },
    "[swift]": {
        "editor.inlayHints.enabled": "off"
    },
    "editor.cursorStyle": "line",
    "editor.lineNumbers": "on",
    "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
    "editor.fontLigatures": false,
    "editor.rulers": [
        120
    ],
    "editor.wordWrapColumn": 120,
    "python.defaultInterpreterPath": "/opt/homebrew/Caskroom/miniforge/base/envs/env1/bin/python",
    "mypy-type-checker.args": ["--ignore-missing-imports"],
    "mypy-type-checker.path": ["/opt/homebrew/Caskroom/miniforge/base/envs/python_3.12/bin/mypy"],
    "vim.leader": "<space>",
    "vim.surround": true,
    "vim.textwidth": 120,
    "vim.highlightedyank.enable": true,
    "vim.highlightedyank.color": "#80a4c2",
    "vim.insertModeKeyBindings": [
    ],
    "vim.normalModeKeyBindings": [
        {
            "before": ["<C-d>"],
            "after": ["<C-d>", "z", "z"],
        },
        {
            "before": ["<C-u>"],
            "after": ["<C-u>", "z", "z"],
        },
        {
            "before": ["n"],
            "after": ["n", "z", "z"],
        },
        {
            "before": ["N"],
            "after": ["N", "z", "z"],
        },
        {
            "before": ["Y"],
            "after": ["y", "$"]
        },
        {
            "before": ["<C-h>"],
            "after": ["<C-w>", "h"]
        },
        {
            "before": ["<C-j>"],
            "after": ["<C-w>", "j"]
        },
        {
            "before": ["<C-k>"],
            "after": ["<C-w>", "k"]
        },
        {
            "before": ["<C-l>"],
            "after": ["<C-w>", "l"]
        },
        {
            "before": ["<leader>", "f"],
            "commands": ["editor.action.formatDocument"],
        },
        {
            "before": ["<leader>", "s"],
            "commands": ["workbench.action.files.save"],
        },
        {
            "before": ["<leader>", "e"],
            "commands": ["editor.action.marker.nextInFiles"],
        },
        {
            "before": ["<leader>", "w"],
            "commands": ["editor.action.marker.prevInFiles"],
        },
        {
            "before": ["<leader>", "E"],
            "commands": ["workbench.files.action.focusFilesExplorer"],
        },
        {
            "before": ["<leader>", "T"],
            "commands": ["workbench.action.terminal.focus"],
        },
        {
            "before": ["<leader>", "G"],
            "commands": ["workbench.scm.focus"],
        },
        {
            "before": ["<leader>", "O"],
            "commands": ["outline.focus"],
        },
        {
            "before": ["<leader>", "D"],
            "commands": ["workbench.view.debug"],
        },
        {
            "before": ["<leader>", "P"],
            "commands": ["togglePeekWidgetFocus"],
        },
        {
            "before": ["<leader>", "C"],
            "commands": ["workbench.panel.chat.view.copilot.focus"],
        },
        {
            "before": ["g", "r"],
            "commands": ["editor.action.goToReferences"],
        },
        {
            "before": ["g", "R"],
            "commands": ["references-view.findReferences"],
        },
        {
            "before": ["g", "j"],
            "commands": ["workbench.action.joinAllGroups"],
        },
        {
            "before": ["g", "X"],
            "commands": ["workbench.action.closeAllEditors"],
        },
    ],
    "vim.visualModeKeyBindings": [
        {
            "before": ["<leader>", "p"],
            "after": ["\"", "_", "d", "P"],
        },
        {
            "before": ["<leader>", "d"],
            "after": ["\"", "_", "d"],
        },
    ],
    "extensions.experimental.affinity": {
        "vscodevim.vim": 1
    },
    "editor.fontSize": 14,
    "terminal.integrated.fontSize": 13,
    "editor.accessibilitySupport": "off",
    "typescript.tsserver.experimental.enableProjectDiagnostics": true,
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "rust-analyzer.inlayHints.chainingHints.enable": false,
    "rust-analyzer.inlayHints.parameterHints.enable": false,
    "rust-analyzer.inlayHints.typeHints.enable": false,
    "rust-analyzer.inlayHints.closingBraceHints.enable": false,
    "rust-analyzer.check.command": "clippy",
    "[rust]": {
        "editor.wordWrap": "off",
    },
    "[python]": {
        "editor.formatOnType": true
    },
    "terminal.integrated.commandsToSkipShell": [
        "language-julia.interrupt"
    ],
    "notebook.lineNumbers": "on",
    "workbench.iconTheme": "material-icon-theme",
    "explorer.compactFolders": false,
    "editor.guides.bracketPairs": true,
    "editor.autoClosingBrackets": "always",
    "editor.autoClosingQuotes": "always",
    "playwright.reuseBrowser": true,
    "svelte.enable-ts-plugin": true,
    "debug.javascript.autoAttachFilter": "disabled",
    "python.languageServer": "Pylance",
    "editor.inlineSuggest.enabled": true,
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": false,
        "scminput": false
    },
    "editor.glyphMargin": true,
    "terminal.external.osxExec": "Warp.app",
    "lldb.library": "/Applications/Xcode.app/Contents/SharedFrameworks/LLDB.framework/Versions/A/LLDB",
    "lldb.launch.expressions": "native",
    "editor.minimap.enabled": false,
    "workbench.colorTheme": "Night Owl (No Italics)",
    "terminal.integrated.fontFamily": "'CaskaydiaCove Nerd Font Mono'",
    "window.title": " ",
    "workbench.sideBar.location": "right",
    "workbench.editor.focusRecentEditorAfterClose": false,
    "workbench.layoutControl.enabled": false,
    "window.commandCenter": false,
    "window.zoomLevel": 1,
    "diffEditor.codeLens": true,
    "continue.enableTabAutocomplete": false,
    "workbench.activityBar.location": "hidden",
    "terminal.integrated.defaultProfile.osx": "zsh"
}
